		<script>
			YUI().use('node', 'event', function(A) {
				A.one('#mainContent').delegate('click', function(event) {
					var currentTarget = event.currentTarget;

					var dropdown = currentTarget.ancestor('.dropdown');

					dropdown.toggleClass('open');

					if (dropdown.hasClass('open')) {
						currentTarget.on('clickoutside', function(event) {
							var _this = this;

							_this.ancestor('.dropdown').removeClass('open');

							_this.detach('clickoutside');
						});
					}
					else {
						currentTarget.detach('clickoutside');
					}
				}, '.dropdown-toggle');
			});
		</script>

