		</div>
	</section>
</div>
<script>
	YUI().use('node', function(A) {
		A.one('#secondaryNav .menu-toggle').on('click', function(event) {
			var currentTarget = event.currentTarget;

			event.preventDefault();

			currentTarget.ancestor('#wrapper').toggleClass('secondary-nav-open').toggleClass('secondary-nav-closed');
			currentTarget.one('.glyphicon').toggleClass('glyphicon-arrow-left').toggleClass('glyphicon-arrow-right');
		});

		A.one('#headerTopBar .menu-toggle').on('click', function(event) {
			var currentTarget = event.currentTarget;

			event.preventDefault();

			currentTarget.ancestor('#wrapper').toggleClass('main-nav-open').toggleClass('main-nav-closed');
		});

		A.one('.side-navigation').delegate('click', function(event) {
			event.preventDefault();
			event.currentTarget.next('.collapse').toggleClass('in');
		}, 'li .side-navigation-dropdown');
	});
</script>