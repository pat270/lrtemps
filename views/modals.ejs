		<script>
			YUI().use('event', 'node', 'transition', function(A) {
				var bodyNode = A.one('body');
				var mainContent = A.one('#mainContent');

				mainContent.delegate('click', function(event) {
					event.preventDefault();
				}, 'a');

				A.one('#modals').delegate('click', function(event) {
					var currentTarget = event.currentTarget;

					currentTarget.ancestor('.modal.fade').on('transitionend', function(event) {
						var _this = this;

						_this.setStyle('display', '');

						bodyNode.removeClass('modal-open');

						_this.detach('transitionend');
					});

					currentTarget.ancestor('.modal').removeClass('in');
				}, '.close-modal');

				A.one('#openModals').delegate('click', function(event) {
					var buttons = A.all('#openModals button');
					var currentTarget = event.currentTarget;
					var modalsArray = A.all('#modals .modal');

					var modal = modalsArray.item(buttons.indexOf(currentTarget));

					if (modal) {
						bodyNode.addClass('modal-open');

						modal.setStyle('display', 'block');

						setTimeout(function() {
							modal.addClass('in');
						}, 50);
					}
				}, 'button');
			});
		</script>
